<div>
<ul>
<% @users.each do |user| %>
  <% if user != current_user %>
    <div class = "user">
      <li><%= "#{user.name} #{user.username} #{user.email}" %>
          <%= link_to("Delete user", user_path(user), method: :delete, data: {confirm: "Are you sure?"}, class: "button alert round") %>
      </li></div>
  <% end %>
<% end %>
</ul>
</div>
